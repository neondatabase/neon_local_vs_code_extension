    "onCommand:neon-local-connect.stopProxy",
    "onCommand:neon-local-connect.clearAuth",
    "onView:neonLocalConnect",
    "onView:neonLocalActions"
  ],
  "main": "./dist/extension.js",
  "icon": "resources/Neon_logo.png",
