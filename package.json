          "when": "view == neonLocalSchema && viewItem == tablesContainer",
          "group": "1_table_create"
        },
        {
          "command": "neonLocal.schema.editRole",
          "when": "view == neonLocalSchema && viewItem == regularRole",
          "group": "1_role_info"
        },
        {
          "command": "neonLocal.schema.changePassword",
          "when": "view == neonLocalSchema && viewItem == regularRole",
          "group": "2_role_modify"
        },
        {
          "command": "neonLocal.schema.resetRolePassword",
          "when": "view == neonLocalSchema && viewItem == neonSuperuserRole",
          "group": "2_role_modify"
        },
        {
          "command": "neonLocal.schema.manageRolePermissions",
          "when": "view == neonLocalSchema && viewItem == regularRole",
          "group": "3_role_permissions"
        },
        {
          "command": "neonLocal.schema.dropRole",
          "when": "view == neonLocalSchema && (viewItem == regularRole || viewItem == neonSuperuserRole)",
          "group": "9_role_delete"
        }
      ]
    }
  },
